{
    "nasm.outputFormat": "elf64",
    "editor.detectIndentation": true,
    "files.associations": {
        "**/toolchain/install/**/*.h": "cpp",
        "**/toolchain/install/**/*.hh": "cpp",
        "**/toolchain/install/**/*.hpp": "cpp",
        "**/toolchain/install/**/*.tcc": "cpp",
        "*.h": "c",
        "*.pl": "prolog",
        "*.pro": "prolog",
        "*.cog_processed": "c",
        "rope": "cpp",
        "*.ld": "linkerscript",
        "*gmsl": "makefile",
        "*.sd": "c",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "string": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "string_view": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "ratio": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "typeinfo": "cpp",
        "variant": "cpp",
        "__verbose_abort": "cpp",
        "__config": "cpp",
        "filesystem": "cpp",
        "ostream": "cpp",
        "print": "cpp",
        "format": "cpp",
        "mutex": "cpp",
        "ranges": "cpp",
        "span": "cpp",
        "text_encoding": "cpp",
        "*.def": "c",
        "*.in": "c",
        "*.tpp": "c"
    },
    "C_Cpp.default.cppStandard": "gnu++23",
    "C_Cpp.default.cStandard": "gnu23",
    "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
    //"C_Cpp.intelliSenseCacheSize": 10240,
    //"C_Cpp.intelliSenseMemoryLimit": 8192,
    //"C_Cpp.workspaceParsingPriority": "highest",
    "C_Cpp.doxygen.generatedStyle": "/**",
    "C_Cpp.doxygen.generateOnType": true,
    "C_Cpp.doxygen.sectionTags": [
        "attention",
        "tparam",
        "param",
        "result",
        "returns",
        "retval",
        "exception",
        "deprecated",
        "warning",
        "note",
        "pre",
        "post",
        "invariant"
    ],
    "C_Cpp.files.exclude": {
        "**/.vscode": true,
        "**/.vs": true,
        //"**/toolchain/src/**": true
        "**/toolchain/**": true
    },
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.autocomplete": "default",
    /* "C_Cpp.exclusionPolicy": "checkFilesAndFolders", */
    "python.analysis.typeCheckingMode": "standard",
    "C/C++ Include Guard.Macro Type": "Filepath",
    "C/C++ Include Guard.Header Extensions": [
        ".h",
        ".hpp",
        ".h++",
        ".hh"
    ],
    "C/C++ Include Guard.Suffix": "_INCLUDED_",
    "C/C++ Include Guard.Path Skip": 2,
    "C/C++ Include Guard.Prefix": "KRONIX_",
    "editor.tabCompletion": "off",
    "C_Cpp.intelliSenseMemoryLimit": 4096,
    "C_Cpp.workspaceParsingPriority": "medium",
    "latex-workshop.latex.autoBuild.run": "never",
    "clang-format.executable": "clang-format",
    "clang-format.style": "file",
    "clang-format.language.c.enable": true,
    "clang-format.language.cpp.enable": true,
    "clang-format.assumeFilename": "${workspaceFolder}/.clang-format",
    "[c]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[makefile]": {
        "editor.insertSpaces": false,
        "editor.defaultFormatter": "eshojaei.mbake-makefile-formatter"
    },
    "mbake.formatOnSave": false,
    "mbake.executablePath": [
        "pipenv",
        "run",
        "mbake"
    ]
}